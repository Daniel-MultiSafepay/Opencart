{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <div class="pull-right">
                <button type="submit" form="form-multisafepay" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
                <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a>
            </div>
            <h1>{{ heading_title }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    
    <div class="container-fluid">
        {% if error_warning %}
            <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
            </div>
            <div class="panel-body">
                <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-multisafepay" class="form-horizontal">
                    <ul class="nav nav-tabs" id="tabs">
                        <li class="active"><a href="#tab-default" data-toggle="tab">Default Store</a></li>
                        {% for store in stores %}
                        	<li><a href="#tab-store-{{ store.store_id }}" data-toggle="tab">Store {{ store.name }}</a></li>
                        {% endfor %}
                    </ul>
                    
                    <div class="tab-content">
                        {% for store in stores %}
                            <div class="tab-pane" id="tab-store-{{ store.store_id }}">
                   
                                <div class="form-group">

                                    {# -- Environment -- #}
                                    <label class="col-sm-2 control-label" for="environment">{{ entry_environment }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_environment_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="environment" class="form-control">
                                            <option value="0" {% if value == '0' %}selected{% endif %}>Live account</option>
                                            <option value="1" {% if value == '1' %}selected{% endif %}>Test account</option>
                                        </select>
                                    </div>
                                    
                                    {# -- Account_Type -- #}
                                    <label class="col-sm-2 control-label" for="input-account-type">{{ entry_environment }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_account_type_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-account-type" class="form-control">
                                            <option value="connect" {% if value == 'connect' %}selected{% endif %}>Connect</option>
                                            <option value="fastcheckout" {% if value == 'fastcheckout' %}selected{% endif %}>Fastcheckout</option>
                                        </select>
                                    </div>

                                    <!--Account settings-->
                                    <label class="col-sm-2 control-label" for="input-merchant_id">{{ entry_multisafepay_merchantid }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_merchant_id_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="merchant_id" class="form-control" />
                                    </div>

                                    <!--siteid settings-->
                                    <label class="col-sm-2 control-label" for="input-siteid">{{ entry_multisafepay_siteid }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_site_id_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="siteid" class="form-control" />
                                    </div>
                                    

                                    <!--site secure code-->
                                    <label class="col-sm-2 control-label" for="input-sitesecurecode">{{ entry_multisafepay_secure_code }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_secure_code_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="sitesecurecode" class="form-control" />
                                    </div>

                                </div>
                                
                                <div class="form-group">
                                   
                                    {# -- Enable_Redirect -- #}
                                    <label class="col-sm-2 control-label" for="input-enable_redirect">{{ entry_multisafepay_redirect_url }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_redirect_url_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name="{{ name }}" id="input-enable_redirect" class="form-control">
                                            <option value="1" {% if value == '1' %}selected{% endif %}>Enabled</option>
                                            <option value="0" {% if value == '0' %}selected{% endif %}>Disabled</option>
                                        </select>
                                    </div>

                                    {# -- days Active -- #}
                                   <label class="col-sm-2 control-label" for="input-sitesecurecode">{{ entry_multisafepay_days_active }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_days_active_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="sitesecurecode" class="form-control" />
                                    </div>
                                    

                                    {# -- Confirm order -- #}
                                    <label class="col-sm-2 control-label" for="environment">{{ entry_confirm_order }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_confirm_order'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name="{{ name }}" id="environment" class="form-control">
                                            <option value="1" {% if value == '1' %}selected{% endif %}>After transaction is paid</option>
                                            <option value="0" {% if value == '0' %}selected{% endif %}>After orderconfirmation</option>
                                        </select>
                                    </div>
                                    
                                    {# -- Min amount -- #}
                                    <label class="col-sm-2 control-label" for="minamount">{{ text_min_amount }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_min_amount_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="minamount" class="form-control" />
                                    </div>
                                    
                                    {# -- Max amount -- #}
                                    <label class="col-sm-2 control-label" for="maxamount">{{ text_max_amount }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_max_amount_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="maxamount" class="form-control" />
                                    </div>
                                    

                                    {# -- Geo-Zone -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-zone">{{ text_all_zones }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_geo_zone_id_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-geo-zone" class="form-control">
                                            <option value="0">{{ text_all_zones }}</option>
                                            {% for row in geo_zones %}
                                                <option value="{{ row.geo_zone_id }}" {% if value == row.geo_zone_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>

                                    {# -- Sorting -- #}
                                    <label class="col-sm-2 control-label" for="input-sort-order">{{ entry_sort_order }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_sort_order_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="input-sort-order" class="form-control" />
                                    </div>
                                </div>                                
                                    
                                <div class="form-group">
                                    
                                    {# -- Enable_fco-button -- #}
                                    <label class="col-sm-2 control-label" for="input-msp">{{ enable_checkout_button }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_enable_checkout_button_connect_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name="{{ name }}" id="input-msp" class="form-control">
                                            <option value="0" {% if value == 0 %}selected{% endif %}>No</option>
                                            <option value="1" {% if value == 1 %}selected{% endif %}>Yes</option>
                                        </select>
                                    </div>

                                    {# -- Enable_B2B field -- #}
                                    <label class="col-sm-2 control-label" for="input-msp">{{ entry_multisafepay_b2b }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_b2b_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name="{{ name }}" id="input-msp" class="form-control">
                                            <option value="0" {% if value == 0 %}selected{% endif %}>{{ text_disabled }}</option>
                                            <option value="1" {% if value == 1 %}selected{% endif %}>{{ text_enabled }}</option>
                                        </select>
                                    </div>

                                    <!--fco tax percentage-->
                                    <label class="col-sm-2 control-label" for="fcotax">{{ entry_multisafepay_fco_tax }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_fco_tax_percent_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="fcotax" class="form-control" />
                                    </div>
                                    
                                    <!--fco free ship amount-->
                                   <label class="col-sm-2 control-label" for="freeship">{{ entry_multisafepay_fco_free_ship }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_fco_free_ship_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}
                                        <input type="text" name= "{{ name }}" value= "{{ value }}"  id="freeship" class="form-control" />
                                    </div>   
                                </div> 
                                        
                                <div class="form-group">
                                    {# -- set initialized status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_initialized }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_initialized_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                        
                                    {# -- set completed status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_completed }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_completed_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                     
                                    {# -- set uncleared status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_uncleared }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_uncleared_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                        
                                    {# -- set reserved status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_reserved }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_reserved_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>         

                                    {# -- set void status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_void }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_void_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>    
                                        
                                    {# -- set declined status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_declined }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_declined_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>                                        
                                        
                                    {# -- set expired status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_expired }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_expired_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                        
                                    {# -- set shipped status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_shipped }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_shipped_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>   

                                    {# -- set refunded status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_refunded }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_refunded_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>   

                                    {# -- set partial_refunded status -- #}
                                    <label class="col-sm-2 control-label" for="input-msp-status">{{ entry_multisafepay_order_status_id_partial_refunded }}</label>
                                    <div class="col-sm-10">
                                        {% set name  = 'payment_multisafepay_order_status_id_partial_refunded_'~store.store_id %}
                                        {% set value = attribute(_context, name) %}

                                        <select name= "{{ name }}" id="input-msp-status" class="form-control">
                                            {% for row in order_statuses %}
                                                <option value="{{ row.order_status_id }}" {% if value == row.order_status_id %}selected{% endif %}>{{ row.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>   
                                </div>
                            </div>                              
                        {% endfor %}
                    </div>                              

					
                    <div class="tab-pane active" id="tab-default">

                        <!--Module status-->
                        <div class="form-group">
                            <label class="col-sm-2 control-label" for="input-status">{{ entry_status }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_status" id="input-status" class="form-control">
                                    {% if payment_multisafepay_status %}
                                        <option value="1" selected="selected">Enabled</option>
                                        <option value="0">Disabled</option>
                                    {% else %}
                                        <option value="1">Enabled</option>
                                        <option value="0" selected="selected">Disabled</option>
                                    {% endif %} 
                                </select>
                            </div>
                        </div>
                            
                        <div class="form-group">
                            <!--MultiSafepay environment-->
                            <label class="col-sm-2 control-label" for="environment">{{ entry_environment }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_environment_0" id="environment" class="form-control">
                                    <option value="0"  {% if payment_multisafepay_environment == '0' %} {{ 'selected' }}{% endif %} >Live Account</option>
                                    <option value="1"  {% if payment_multisafepay_environment == '1' %} {{ 'selected' }}{% endif %} >Test Account</option>
                                </select>
                            </div>

                            <!--MultiSafepay account type-->
                            <label class="col-sm-2 control-label" for="account">{{ entry_account_type }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_account_type_0" id="account" class="form-control">
                                    <option value="connect"      {% if payment_multisafepay_account_type == 'connect'      %} {{ 'selected' }}{% endif %} >Connect</option>
                                    <option value="fastcheckout" {% if payment_multisafepay_account_type == 'fastcheckout' %} {{ 'selected' }}{% endif %} >FastCheckout</option>
                                </select>
                            </div>

                            <!--Account settings-->
                            <label class="col-sm-2 control-label" for="accountid"><span>{{ entry_multisafepay_merchantid }}</span></label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_merchant_id_0" value="{{ payment_multisafepay_merchant_id }}" id="input-api-key" class="form-control" />
                            </div>

                            <!--siteid settings-->
                            <label class="col-sm-2 control-label" for="siteid"><span>{{ entry_multisafepay_siteid }}</span></label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_site_id_0" value="{{ payment_multisafepay_site_id }}" id="siteid" class="form-control" />
                            </div>

                            <!--site secure code-->
                            <label class="col-sm-2 control-label" for="sitesecurecode"><span>{{ entry_multisafepay_secure_code }}</span></label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_secure_code_0" value="{{ payment_multisafepay_secure_code }}" id="sitesecurecode" class="form-control" />
                            </div>
                        </div>


                        <div class="form-group">                        
                        
                            <!--MultiSafepay enable redirect-->
                            <label class="col-sm-2 control-label" for="redirect">{{ entry_multisafepay_redirect_url }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_redirect_url_0" id="redirect" class="form-control">
                                    <option value="true"    {% if payment_multisafepay_redirect_url == 'true'  %} {{ 'selected' }}{% endif %}>Enabled</option>
                                    <option value="false"   {% if payment_multisafepay_redirect_url == 'false' %} {{ 'selected' }}{% endif %}>Disabled</option>
                                </select>
                            </div>
                       
                            <!--Days active -->
                            <label class="col-sm-2 control-label" for="daysactive"><span>{{ entry_multisafepay_days_active }}</span></label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_days_active_0" value="{{ payment_multisafepay_days_active }}" id="daysactive" class="form-control" />
                            </div>
                           
                            <label class="col-sm-2 control-label" for="environment">{{ entry_confirm_order }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_confirm_order_0" id="environment" class="form-control">
                                    <option value="0" {% if payment_multisafepay_confirm_order ==  0 %} {{ 'selected' }}{% endif %}>Before transaction</option>
                                    <option value="1" {% if payment_multisafepay_confirm_order ==  1 %} {{ 'selected' }}{% endif %}>After paid transaction</option>
                                </select>
                            </div>

                            <label class="col-sm-2 control-label" for="minamount"><span>{{ text_min_amount }}</span></label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_min_amount_0" value="{{ payment_multisafepay_min_amount }}" id="minamount" class="form-control" />
                            </div>

                            <label class="col-sm-2 control-label" for="maxamount"><span>{{ text_max_amount }}</span></label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_max_amount_0" value="{{ payment_multisafepay_max_amount }}" id="maxamount" class="form-control" />
                            </div>
                            
                            <label class="col-sm-2 control-label" for="input-geo-zone">{{ text_all_zones }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_geo_zone_id_0" id="input-geo-zone" class="form-control">
                                   <option value="0">{{ text_all_zones }}</option>
                                    {% for geo_zone in geo_zones %}
                                        {% if geo_zone.geo_zone_id  == payment_multisafepay_geo_zone_id %}
                                            <option value="{{ geo_zone.geo_zone_id }}" selected="selected">{{ geo_zone.name }}</option>
                                        {% else %}
                                            <option value="{{ geo_zone.geo_zone_id }}">{{ geo_zone.name }}</option>
                                        {% endif %}
                                    {% endfor %}
                                </select>
                            </div>


                            <!--Sorting-->
                            <label class="col-sm-2 control-label" for="input-sort-order">{{ entry_sort_order }}</label>
                            <div class="col-sm-10">
                                <input type="text" name="payment_multisafepay_sort_order_0" value="{{ payment_multisafepay_sort_order }}" placeholder="{{ entry_sort_order }}" id="input-sort-order" class="form-control" />
                            </div>
                        </div>
                            
                        <div class="form-group">                        
                            <!--MultiSafepay fco button-->
                            <label class="col-sm-2 control-label" for="input-msp">{{ enable_checkout_button }}</label>
                            <div class="col-sm-10">
                                <select name="payment_multisafepay_enable_checkout_button_connect_0" id="fcobutton" class="form-control">
                                    <option value="true"   {% if payment_multisafepay_enable_checkout_button_connect ==  'true'   %} {{ 'selected' }}{% endif %}>{{ yes }}</option>
                                    <option value="false"   {% if payment_multisafepay_enable_checkout_button_connect == 'false'  %} {{ 'selected' }}{% endif %}>{{ no }}</option>
                                </select>
